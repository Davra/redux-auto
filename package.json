{
  "name": "redux-auto",
  "version": "1.6.1",
  "description": "automatically generate stories and actions from your folder and file structure",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:go": "npm test -- --watch --coverage",
    "test:cover": "npm test -- --coverage --no-cache",
    "test:watch": "npm test -- --watch"
  },
  "jest": {
    "verbose": true,
    "browser": true,
    "testPathIgnorePatterns": [
      "<rootDir>/example/",
      "<rootDir>/node_modules/"
    ],
    "haste": {
      "providesModuleNodeModules": [
        ".*"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/codemeasandwich/redux-auto.git"
  },
  "keywords": [
    "redux",
    "thunk",
    "middleware",
    "reducers",
    "actions",
    "loading",
    "webpack"
  ],
  "author": "Brian Shannon (http://www.codemeasandwich.com)",
  "license": "MPL",
  "bugs": {
    "url": "https://github.com/codemeasandwich/redux-auto/issues"
  },
  "homepage": "https://github.com/codemeasandwich/redux-auto#readme",
  "devDependencies": {
    "babel-jest": "^20.0.3",
    "babel-preset-es2015": "^6.24.1",
    "coveralls": "^2.13.1",
    "faker": "^4.1.0",
    "jest": "^20.0.4",
    "redux": "^3.6.0"
  }
}
